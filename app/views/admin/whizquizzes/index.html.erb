<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">        
        <%= link_to "", new_admin_whizquiz_path, class: "btn btn-primary btn-xs pull-right glyphicon glyphicon-plus", title: "Add Whizquiz" , style: "color:#FFF;margin-top:-3px;" %>
        <h3 class="panel-title" , style="color:#000000;" ><b>Whizquizzes</b></h3>
      </div>
      <div class="panel-body">
        <% if @assets.count > 0 %>
          <%= datagrid_table(@report, @assets) %>
          <%= paginate @assets %>
           <%= page_entries_info @assets %>              
        <% else %>
          <p class="alert">There are no results to display . </p>
        <% end %>
      </div>
    </div>    
  </div>  
 
</div>
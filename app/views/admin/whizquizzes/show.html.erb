<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-lg-6 col-lg-offset-3">
    <div class="panel panel-default">
      <div class="panel-heading">        
        <h3 class="panel-title" style="color: #000000"><b>Question No <%= @whizquiz.id %></b></h3>
      </div>
      <div class="panel-body">


<p>
  <strong>Question:</strong>
  <%= @whizquiz.question %>
</p>
<p>
  <strong>Answer:</strong>
  <%= @whizquiz.answer %>
</p>
<p>
  <strong>Created At:</strong>
  <%= @whizquiz.created_at.to_date %>
</p>
<p>
  <strong>Updated At:</strong>
  <%= @whizquiz.updated_at.to_date %>
</p>


<div class="form-group clearfix">
    <div class="col-sm-offset-3 col-sm-9">
      <%= link_to 'Edit', edit_admin_whizquiz_path(@whizquiz), class: "btn btn-primary btn-sm" %> <%= link_to "Back", admin_whizquizzes_path, class: "btn btn-default btn-sm" %>
    </div>
  </div>
      </div>
    </div>
  </div>
</div>
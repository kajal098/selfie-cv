<div class="row">
  <div class="col-lg-9">
    <div class="panel panel-default">
      <div class="panel-heading">        
        <%= link_to "", new_admin_course_path, class: "btn btn-primary btn-xs pull-right glyphicon glyphicon-plus", title: "Add Course" , style: "color:#FFF;margin-top:-3px;" %>
        <h3 class="panel-title" , style="color:#000000;" ><b>Courses</b></h3>
      </div>
      <div class="panel-body">
      <% if @assets.count > 0 %>
        <%= datagrid_table(@report, @assets) %>
        <%= paginate @assets %>
        <%= page_entries_info @assets %>              
        <% else %>
                <p class="alert">There are no results to display . </p>
<% end %>

      </div>
    </div>    
  </div>  
  <div class="col-lg-3">
    <div class="panel panel-default">
      <div class="panel-heading">        
        <h3 class="panel-title" , style="color:#000000;" ><b>Filter</b></h3>
      </div>
      <div class="panel-body"><%= render "admin/shared/filters" %></div>
    </div>
  </div>
</div>
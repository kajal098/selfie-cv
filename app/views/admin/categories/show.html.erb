<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-lg-6 col-lg-offset-3">
    <div class="panel panel-default">
      <div class="panel-heading">        
        <h3 class="panel-title" style="color: #000000"><b>Category <%= @category.id %></b></h3>
      </div>
      <div class="panel-body">


<p>
  <strong>Name:</strong>
  <%= @category.name %>
</p>
<p>
  <strong>Created At:</strong>
  <%= @category.created_at.to_date %>
</p>
<p>
  <strong>Updated At:</strong>
  <%= @category.updated_at.to_date %>
</p>

<div class="form-group clearfix">
    <div class="col-sm-offset-3 col-sm-9">
      <%= link_to 'Edit', edit_admin_category_path(@category), class: "btn btn-primary btn-sm" %> <%= link_to "Back", admin_categories_path, class: "btn btn-default btn-sm" %>
    </div>
  </div>
      </div>
    </div>
  </div>
</div>
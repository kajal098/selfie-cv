<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-lg-6 col-lg-offset-3">
    <div class="panel panel-default">
      <div class="panel-heading">        
        <h3 class="panel-title" style="color: #000000"><b>Question No : <%= @marketiq.id %></b></h3>
      </div>
      <div class="panel-body">


<p>
  <strong>Question:</strong>
  <%= @marketiq.question %>
</p>

<p>
  <strong>Option A:</strong>
  <%= @marketiq.option_a %>
</p>

<p>
  <strong>Option B:</strong>
  <%= @marketiq.option_b %>
</p>

<p>
  <strong>Option C:</strong>
  <%= @marketiq.option_c %>
</p>

<p>
  <strong>Option D:</strong>
  <%= @marketiq.option_d %>
</p>

<p>
  <strong>Answer:</strong>
  <%= @marketiq.answer %>
</p>

<div class="form-group clearfix">
    <div class="col-sm-offset-3 col-sm-9">
      <%= link_to 'Edit', edit_admin_marketiq_path(@marketiq), class: "btn btn-primary btn-sm" %> <%= link_to "Back", admin_marketiqs_path, class: "btn btn-default btn-sm" %>
    </div>
  </div>
      </div>
    </div>
  </div>
</div>
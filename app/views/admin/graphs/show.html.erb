<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-lg-6 col-lg-offset-3">
    <div class="panel panel-default">
      <div class="panel-heading">        
        <h3 class="panel-title" style="color: #000000"><b><%= @graph.id %></b></h3>
      </div>
      <div class="panel-body">

<% if @graph.company_stock_id.present? %>
<p>
  <strong>Country:</strong>
  <%= @graph.company_stock.stock_country_id %>
</p>
<% end %>

<% if @graph.industry_id.present? %>
<p>
  <strong>Industry:</strong>
  <%= @graph.industry.name %>
</p>
<% end %>

<p>
  <strong>company_code:</strong>
  <%= @graph.company_code %>
</p>

<p>
  <strong>Created At:</strong>
  <%= @graph.created_at.to_date %>
</p>
<p>
  <strong>Updated At:</strong>
  <%= @graph.updated_at.to_date %>
</p>

<div class="form-group clearfix">
    <div class="col-sm-offset-3 col-sm-9">
      <%= link_to 'Edit', edit_admin_graph_path(@graph), class: "btn btn-primary btn-sm" %> <%= link_to "Back", admin_graphs_path, class: "btn btn-default btn-sm" %>
    </div>
  </div>
      </div>
    </div>
  </div>
</div>